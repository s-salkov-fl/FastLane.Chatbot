{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"Browser": {
		"Path": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe",
		"ProfileConfigName": "TestProfile1"
	},
	"WhatsAppPageExpressions": {
		"ContactContainer": "xpath///div[@role='grid']/div[@role='listitem' and .//div[@role='gridcell']]",
		"SpanChatName": "xpath/.//div[@role='gridcell']/div/div/span",
		"SpanUnreadNumber": "xpath/.//div[@role='gridcell']/span/div/span",
		"SearchInputXpath": "xpath///div[@contenteditable and @role='textbox']",
		"UserCommentBoxXpath": "xpath///div[@role='application']/div[@role='row' and .//*[contains(@class,'message-in')] and .//div[contains(@class,'copyable-text')]]",
		"BotCommentBoxXpath": "xpath///div[@role='application']/div[@role='row' and .//*[contains(@class,'message-out')] and .//div[contains(@class,'copyable-text')]]",
		"UserAndBotCommentBoxXpath": "xpath///div[@role='application']/div[@role='row' and .//*[contains(@class,'message-in') or contains(@class,'message-out')] and .//div[contains(@class,'copyable-text')]]",
		"UserMessageMarker": "xpath/.//*[contains(@class,'message-in')]",
		"BotMessageMarker": "xpath/.//*[contains(@class,'message-out')]",
		"ChatInputXpath": "xpath///div[@id='main']//div[@contenteditable and @role='textbox']"
	},
	"FaceBookPageExpressions": {
		"ChatContainer": "xpath///div[@role='navigation']//div[@role='grid']",
		"ContactContainer": "xpath///div[@role='navigation']//div[@role='grid']//div[@role='row']/div[@role='gridcell'][1]",
		"ChatIdLink": "xpath/.//a[@role='link']",
		"ChatId": "xpath/.//a[@role='link']/@href",
		"ContactName": "xpath/.//div[@role='img']/@aria-label",
		"MessageExistSign": "xpath/.//div[@role='button' and @aria-label='Mark as read']/div",
		"ChatMessageContainer": "xpath///div[@role='row' and contains(@style,'--comment')]",
		"ChatInput": "xpath///div[@role='main']//div[@role='textbox' and @aria-label='Message']"
	},
	"TikTokPageExpressions": {
		"ChatContainer": "xpath///*[@id='main-content-messages']",
		"ContactContainer": "xpath///",
		"ChatMessageContainerPattern": "div[@data-e2e='chat-item' and .//a[contains(@href,'@{0}')]]//div[contains(@class,'DivTextContainer')]",
		"ChatInput": "xpath///"
	},
	"WhatsApp": {
		"TimeOutContactsRefreshMs": 5000,
		"GeneralMutateFailCrutchWaitMs": 1000
	},
	"Facebook": {
		"NewStateEventPollingPeriodMs": 5000,
		"GeneralMutateFailCrutchWaitMs": 2000
	},
	"TikTok": {
		"NewStateEventPollingPeriodMs": 5000
	}
}
